<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="DickimawBooks/DatatoolTk"
  location="applications"/>

  <shortcut
     name="DatatoolTk"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="$INSTALL_PATH/bin/datatooltk-gui"
     commandLine=""
     workingDirectory=""
     description="GUI to accompany datatool.sty"
     iconFile="$INSTALL_PATH/lib/icons/datatooltk-logosmall.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="false"
     KdeSubstUID="false"
     initialState="normal">
     <createForPack name="Base"/>
   </shortcut>

   <shortcut
     name="Uninstaller"
     programGroup="no"
     desktop="no"
     applications="no"
     startMenu="no"
     startup="no"
     target="java"
     commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0"
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="DatatoolTk uninstaller">
     <createForPack name="Base" />
   </shortcut>

</shortcuts>
