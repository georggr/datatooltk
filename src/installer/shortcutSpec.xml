<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="DickimawBooks\DatatoolTk"
                location="applications||startMenu"/>
  <shortcut
    name="Start DatatoolTk"
    target="$INSTALL_PATH\bin\datatooltk-gui.bat"
    commandLine=""
    workingDirectory="$INSTALL_PATH"
    description="GUI to accompany datatool.sty"
    iconFile="$INSTALL_PATH\lib\icons\datatooltk.ico"
    iconIndex="0"
    initialState="noShow||normal||maximized||minimized"
    programGroup="yes||no"
    desktop="yes||no"
    applications="yes||no"
    startMenu="yes||no"
    startup="yes||no">

    <createForPack name="Base"/>
  </shortcut>

</shortcuts>
